{
  "expo": {
    "name": "soil_monitor_app",
    "slug": "soil-monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dirt.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dirt.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devramoscesar.soilmonitor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dirt.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8388a38c-745d-4a86-95d3-310deb09b201"
      }
    },
    "owner": "devramoscesar"
  }
}
